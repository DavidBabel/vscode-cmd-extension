{
    "name": "Extra Commands",
    "displayName": "Extra Commands",
    "description": "Simple extension for duplicating code inline and fast deploy",
    "version": "0.0.1",
    "publisher": "alqiu",
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.doDuplicate",
        "onCommand:extension.doFastDeploy"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.doDuplicate",
            "title": "Duplicate Inline"
        }],
        "keybindings": [{
            "command": "extension.doDuplicate",
            "key": "alt+d"
        },
        {
            "command": "extension.doFastDeploy",
            "key": "alt+f"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}